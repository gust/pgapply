    "jasmine": "^2.5.3",
    "raw-loader": "^0.5.1",
    "typescript": "~2.1.1",
    "typings": "^1.4.0",
    "webpack": "^2.2.0",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "nodegit": "^0.16.0",
    "pg-promise": "^5.3.4"
